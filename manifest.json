{
  "manifest_version": 2,
  "name": "Wiki Extension",
  "version": "1.0",
  "description": "Open a random wiki tab",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "commands": {
    "open_website": {
      "suggested_key": {
        "default": "Ctrl+I",
        "mac": "MacCtrl+Shift+O"
      },
      "description": "Open the wiki page"
    }
  },
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "icon.png"
  }
}